<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n">

 <object name="user" meta_type="CMF Action Category">
  <object name="go_to_school_term" meta_type="CMF Action" i18n:domain="artistic.content" insert-before="logout">
   <property name="title" i18n:translate="">Go to school term</property>
   <property name="description" i18n:translate="">Go to school term</property>
   <property name="url_expr">string:${portal_url}/@@go_to_school_term</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="artistic.content: Is School"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>

 <object name="user" meta_type="CMF Action Category">
  <object name="go_to_old_admin" meta_type="CMF Action" i18n:domain="artistic.content" insert-before="logout">
   <property name="title" i18n:translate="">Go to old admin platform</property>
   <property name="description" i18n:translate="">Go to old admin platform</property>
   <property name="url_expr">string:${portal_url}/@@go_to_old_admin</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="Manage portal"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>

</object>

